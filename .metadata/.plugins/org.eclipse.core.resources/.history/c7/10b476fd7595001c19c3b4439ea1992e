package othello;

import players.Player;

public class Game {
	
	private final Frame[][] frame = new  Frame[8][8];
	
	private Player player1;
	private Player player2;
	
	public Game(Player player1, Player player2) {
		super();
		
		for (int i = 0 ; i < 8 ; i++) {
			for (int p = 0 ; p < 8 ; p++) {
				frame[i][p] = new Frame();
			}
		}
		
		this.player1 = player1;
		this.player2 = player2;
	}
	
	

}
